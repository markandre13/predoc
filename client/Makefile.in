all: @JAVAC@

jikes:
	@clear
	@echo "--------------------------------------------------------------------------------"
	jikes +F +$$ -d class predoc.java

javac:
	@clear
	@echo "--------------------------------------------------------------------------------"
	rm -f `find . -name "*.class"`
	javac -d class predoc.java
